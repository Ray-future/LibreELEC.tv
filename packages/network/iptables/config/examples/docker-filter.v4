*filter
-A FORWARD -j DOCKER-USER
-A DOCKER-USER -m conntrack --ctstate RELATED,ESTABLISHED -j RETURN                       
-A DOCKER-USER -s 10.0.0.0/8 -m conntrack --ctstate NEW -j RETURN                         
-A DOCKER-USER -s 172.16.0.0/12 -m conntrack --ctstate NEW -j RETURN                      
-A DOCKER-USER -s 192.168.0.0/16 -m conntrack --ctstate NEW -j RETURN                     
-A DOCKER-USER -j DROP
COMMIT
